public class ContaCorrente extends Conta {

	private boolean statusChequeEspecial;
	private int limiteChequeEspecial;
	
	public ContaCorrente (int LimiteSaque, int NumeroConta, int Saldo, boolean StatusChequeEspecial, int LimiteChequeEspecial){
		super (LimiteSaque, NumeroConta, Saldo);
		numeroConta = NumeroConta;
		saldo = Saldo;
		statusChequeEspecial = StatusChequeEspecial;
		limiteChequeEspecial = LimiteChequeEspecial;
		tipoConta = TipoConta.corrente;
	}
	
	public boolean GetStatusChequeEspecial () {
		return statusChequeEspecial;
	}
	
	public int GetLimiteChequeEspecial () {
		return limiteChequeEspecial;
	}
}
